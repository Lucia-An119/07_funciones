#include<iostream>
using namespace std;
int factorial(int);//prototipo
int main(){
    int m,n,c;
    cout<<"Ingrese valor de m: ";
    cin>>m;
    cout<<"Ibgrese valor de n: ";
    cin>>n;
    c=factorial(m)/(factorial(n)*factorial(m-n));
    cout<<c;
return 0;
}
int factorial (int n){
    int f;
    for(int i=2;i<=n;i++)
        f=f*i;
    return f;
}

